CREATE DATABASE Student_Record;
USE Student_Record;

CREATE TABLE student(
id INT PRIMARY KEY,
roll_no INT,
name  VARCHAR(40),
marks INT,
grade VARCHAR(60)
);

INSERT INTO student VALUES(101 , 01 , "AKASH" , 30 , 'A');
INSERT INTO student VALUES(102 , 02 , "AKASH" , 35 , 'A');
INSERT INTO student VALUES(103 , 03 , "AKASH" , 60 , 'A');
INSERT INTO student VALUES(105 , 05 , "AKASH" , 50 , 'A');
INSERT INTO student VALUES (109, 4, 'SUMIT', 156, 'C');


SELECT * FROM student WHERE name  =  'SUMIT';
